    "%%manim -ql MyScene\n",
    "\n",
    "class MyScene(Scene):\n",
    "    def construct(self):\n",
    "        # 设置背景颜色为米黄色\n",
    "        self.camera.background_color = \"#F5DEB3\"  # 米黄色的 HEX 颜色\n",
    "        \n",
    "        text = Text(\"Hello, Manim!\")\n",
    "        self.play(Write(text))\n",
    "        self.wait()"
