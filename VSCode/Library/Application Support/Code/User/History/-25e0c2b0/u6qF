EN_ONE = enOnecol
EN_TWO = enTwocol
CN_ONE = cnOnecol
CN_TWO = cnTwocol
TEX_FILE = enOnecol, enTwocol, cnOnecol, cnTwocol

XE_COMPILE = xelatex -interaction=nonstopmode -shell-escape 

e1:
	$(XE_COMPILE) $(EN_ONE)
	biber $(EN_ONE)
	$(XE_COMPILE) $(EN_ONE)
	$(XE_COMPILE) $(EN_ONE)
	latexmk -c $(EN_ONE)