// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+shift+t",
    "ommand": "openInTerminal",
    "when": "editorFocus"
  },
  {
    "key": "alt+l",
    "command": "cursorEnd",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+q",
    "command": "cursorHome",
    "when": "editorTextFocus"
  },
    // Option + A for moving left
    {
      "key": "alt+a",
      "command": "cursorLeft",
      "when": "editorTextFocus"
    },
    // Option + S for moving down
    {
      "key": "alt+s",
      "command": "cursorDown",
      "when": "editorTextFocus"
    },
    // Option + D for moving right
    {
      "key": "alt+d",
      "command": "cursorRight",
      "when": "editorTextFocus"
    },
    // Option + W for moving up
    {
      "key": "alt+w",
      "command": "cursorUp",
      "when": "editorTextFocus"
    },
    // Option + J for moving candidate selection down
    {
      "key": "alt+j",
      "command": "selectNextSuggestion",
      "when": "suggestWidgetVisible && textInputFocus"
    },
    // Option + K for moving candidate selection up
    {
      "key": "alt+k",
      "command": "selectPrevSuggestion",
      "when": "suggestWidgetVisible && textInputFoucus"
    }
]